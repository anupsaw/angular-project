{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "sz-soochi": [
        "dist/sz-soochi"
      ],
      "sz-soochi/*": [
        "dist/sz-soochi/*"
      ],
      "@sz-soochi": [
        "projects/sz-soochi"
      ],
      "@sz-soochi/*": [
        "projects/sz-soochi/*"
      ],
      "@sz-forms": [
        "projects/sz-forms"
      ],
      "@sz-forms/*": [
        "projects/sz-forms/*"
      ],
      "sz-forms": [
        "dist/sz-forms"
      ],
      "sz-forms/*": [
        "dist/sz-forms/*"
      ],
      "@sz-input": [
        "projects/sz-input"
      ],
      "@sz-input/*": [
        "projects/sz-input/*"
      ],
      "angular-material-ext": [
        "dist/angular-material-ext"
      ],
      "angular-material-ext/*": [
        "dist/angular-material-ext/*"
      ]
    }
  }
}