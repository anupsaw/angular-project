{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "sahaz",
  "projects": {
    "sahez": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sahez",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": false,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "sahez:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "sahez:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sahez:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "sahez:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sahez:serve:production"
            }
          }
        }
      }
    },
    "mool": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "export": true,
          "viewEncapsulation": "None",
          "changeDetection": "OnPush"
        }
      },
      "root": "sahaz/mool",
      "sourceRoot": "sahaz/mool/src",
      "prefix": "sz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "sahaz/mool/tsconfig.lib.json",
            "project": "sahaz/mool/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "sahaz/mool/src/test.ts",
            "tsConfig": "sahaz/mool/tsconfig.spec.json",
            "karmaConfig": "sahaz/mool/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "sahaz/mool/tsconfig.lib.json",
              "sahaz/mool/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ansh": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "inlineStyle": true,
          "export": true,
          "viewEncapsulation": "None",
          "changeDetection": "OnPush"
        }
      },
      "root": "sahaz/ansh",
      "sourceRoot": "sahaz/ansh/src",
      "prefix": "sz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "sahaz/ansh/tsconfig.lib.json",
            "project": "sahaz/ansh/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "sahaz/ansh/src/test.ts",
            "tsConfig": "sahaz/ansh/tsconfig.spec.json",
            "karmaConfig": "sahaz/ansh/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "sahaz/ansh/tsconfig.lib.json",
              "sahaz/ansh/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "kand": {
      "projectType": "library",
      "schematics": {
        "@schematics/angular:component": {
          "inlineStyle": true,
          "export": true,
          "viewEncapsulation": "None",
          "changeDetection": "OnPush"
        }
      },
      "root": "sahaz/kand",
      "sourceRoot": "sahaz/kand/src",
      "prefix": "sz",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "sahaz/kand/tsconfig.lib.json",
            "project": "sahaz/kand/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "sahaz/kand/src/test.ts",
            "tsConfig": "sahaz/kand/tsconfig.spec.json",
            "karmaConfig": "sahaz/kand/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "sahaz/kand/tsconfig.lib.json",
              "sahaz/kand/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "sahez"
}