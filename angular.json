      }
    },
    "AngularMaterialExt": {
      "projectType": "library",
      "root": "projects/angular-material-ext",
      "sourceRoot": "projects/angular-material-ext/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
