{
  "name": "sahez",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot & npm run serve-scss:kand",
    "build": "ng build --aot",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev-server": "file-server",
    "serve-scss:kand": "./node_modules/.bin/scss-bundle -p ./sahaz/kand/scss-bundle.config.json --watch",
    "analyze": "source-map-explorer dist/sahez/vendor-es5.js dist/sahez/vendor-es5.js.map",
    "build-scss:mool": "./node_modules/.bin/scss-bundle -p ./sahaz/mool/scss-bundle.config.json",
    "build-scss:kand": "./node_modules/.bin/scss-bundle -p ./sahaz/kand/scss-bundle.config.json",
    "build-scss:ansh": "./node_modules/.bin/scss-bundle -p ./sahaz/ansh/scss-bundle.config.json",
    "build:mool": "ng lint --project mool && ng build --project mool && npm run build-scss:mool",
    "build:ansh": "ng lint --project mool && ng build --project mool && npm run build-scss:ansh",
    "build:kand": "ng lint --project mool && ng build --project mool && npm run build-scss:kand"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.3",
    "@angular/cdk": "^8.1.4",
    "@angular/common": "~8.0.3",
    "@angular/compiler": "~8.0.3",
    "@angular/core": "~8.0.3",
    "@angular/elements": "^8.2.0",
    "@angular/forms": "~8.0.3",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~8.0.3",
    "@angular/platform-browser-dynamic": "~8.0.3",
    "@angular/router": "~8.0.3",
    "@webcomponents/custom-elements": "^1.2.4",
    "@webcomponents/shadydom": "^1.6.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.6",
    "@angular-devkit/build-ng-packagr": "~0.800.6",
    "@angular/cli": "~8.0.6",
    "@angular/compiler-cli": "~8.0.3",
    "@angular/language-service": "~8.0.3",
    "@sahaz/file-server": "^1.0.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "eslint": "^6.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "scss-bundle": "^3.0.1",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  }
}
